#include 'protheus.ch'

User Function M030INC()
  Local aArea := GetArea() // abre a area SA1

  if PARAMIXB <> 3
    MsgInfo('Você acabou de inserir com sucesso o cliente: '+Chr(10)+Chr(13)+;
    M->A1_NOME)

    RecLock('SA1', .F.)
      SA1->A1_XUSRINC := RetCodUsr() // __cUserID
    MsUnlock()
  endif

  Restarea(aArea) // libera a area
return
